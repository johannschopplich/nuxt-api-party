{
  "name": "nuxt-api-party",
  "type": "module",
  "version": "3.2.0",
  "packageManager": "pnpm@10.17.0",
  "description": "Securely connect to any API with a server proxy and generated composables",
  "author": "Johann Schopplich <hello@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://nuxt-api-party.byjohann.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/nuxt-api-party.git"
  },
  "bugs": "https://github.com/johannschopplich/nuxt-api-party/issues",
  "keywords": [
    "api",
    "fetch",
    "nuxt",
    "open-api",
    "openapi",
    "query"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "default": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "types": "./dist/types.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxt dev playground",
    "dev:build": "nuxt build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt prepare playground && nuxt prepare test/fixture",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:types": "tsc --noEmit",
    "release": "bumpp"
  },
  "peerDependencies": {
    "openapi-typescript": "^5 || ^6 || ^7"
  },
  "peerDependenciesMeta": {
    "openapi-typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "@nuxt/kit": "^4.1.2",
    "defu": "^6.1.4",
    "h3": "^1.15.4",
    "jiti": "^2.5.1",
    "ofetch": "^1.4.1",
    "ohash": "^2.0.11",
    "openapi-typescript-helpers": "0.0.13",
    "pathe": "^2.0.3",
    "scule": "^1.3.0",
    "ufo": "^1.6.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.3.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/test-utils": "^3.19.2",
    "@types/node": "^24.5.2",
    "bumpp": "^10.2.3",
    "eslint": "^9.35.0",
    "nuxt": "^4.1.2",
    "openapi-typescript": "^7.9.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.7"
  }
}
