{
  "name": "nuxt-api-party",
  "type": "module",
  "version": "1.1.2",
  "packageManager": "pnpm@8.15.7",
  "description": "Securely connect to any API with a server proxy and dynamic composables",
  "author": "Johann Schopplich <pkg@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://nuxt-api-party.byjohann.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/nuxt-api-party.git"
  },
  "bugs": "https://github.com/johannschopplich/nuxt-api-party/issues",
  "keywords": [
    "nuxt",
    "nuxt3",
    "api",
    "composable",
    "query",
    "fetch"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground && nuxi prepare test/fixture",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:types": "tsc --noEmit",
    "release": "bumpp --commit --push --tag"
  },
  "dependencies": {
    "@nuxt/kit": "^3.11.2",
    "defu": "^6.1.4",
    "ofetch": "^1.3.4",
    "ohash": "^1.1.3",
    "openapi-typescript": "^7.0.0-next.8",
    "openapi-typescript-helpers": "^0.0.7",
    "pathe": "^1.1.2",
    "scule": "^1.3.0",
    "ufo": "^1.5.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.14.0",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/test-utils": "^3.12.1",
    "@types/node": "^20.12.7",
    "bumpp": "^9.4.0",
    "eslint": "^9.0.0",
    "nuxt": "^3.11.2",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  }
}
